{
  "name": "agent-package",
  "version": "1.0.0",
  "description": "AI-powered agent package for automated testing and analysis",
  "type": "module",
  "main": "agent.js",
  "scripts": {
    "start": "node agent.js",
    "test": "jest",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "testing",
    "analysis",
    "performance",
    "monitoring"
  ],
  "author": "WISPR Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wispr/agent-package.git"
  },
  "bugs": {
    "url": "https://github.com/wispr/agent-package/issues"
  },
  "homepage": "https://wispr.ai/agent-package",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "express": "^5.2.1",
    "cors": "^2.8.5",
    "webdriverio": "^9.22.0",
    "xml2js": "^0.6.2",
    "playwright": "^1.49.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.6",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  }
}